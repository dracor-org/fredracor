	<front>
		<docTitle>
			<titlePart type="main">PROLOGUE</titlePart>
		</docTitle> 
		<docDate when="1830">1830.</docDate>
		<docAuthor>PAR VICTOR HUGO.</docAuthor><!--@bio="hugo"-->
		<div type="docImprint">
			<div type="printer">
                    <p>PARIS, MAME et DELAUNAY-VALLÉE, Libraires, rue Guénégaud, n°25.</p>
                </div><!--@id="Ribou"-->
		</div>
	<castList>
                <head>PERSONNAGES</head>
		<castItem>
                    <role corresp="#jupiter">JUPITER</role><!--@sex="1"--><!--@type="D"--><!--@statut="dieu"--><!--@age="A"--><!--@stat_amour="néant"-->.</castItem>
		<castItem>
                    <role corresp="#comedie">COMÉDIE</role><!--@sex="1"--><!--@type="A"--><!--@statut="allégorie"--><!--@age="A"--><!--@stat_amour="néant"-->.</castItem>
		<castItem>
                    <role corresp="#tragedie">TRAGÉDIE</role><!--@sex="1"--><!--@type="A"--><!--@statut="allégorie"--><!--@age="A"--><!--@stat_amour="néant"-->.</castItem>
	</castList>
	<!--TODO: usage of set in correct place but with unknown attributes <set location="Lieu mythologique" country="pays mythologique" periode="temps mythologiques" gps="[N/A]"/>-->
	</front>
	<note>"Théâtre en Liberté" de Victor Hugo, Paris, 1886. pp. 3-8</note>
<body>
<div type="acte" n="1">
                <head>PROLOGUE</head>
<div type="scene" n="1">
                    <head/>
<sp who="#jupiter">
                        <speaker>JUPITER.</speaker>
                        <l n="1">Vous, Tragédie, et toi, Comédie, approchez.</l>
                        <l n="2">J'ai là le bien, le mal, les exploits, les péchés.</l>
                        <l n="3">Demandez. Je prétends vous doter l'une et l'autre.</l>
                        <l n="4" part="I">Parlez. Que voulez-vous toutes deux ?</l><!--@syll="9"-->
                    </sp><!--@who="JUPITER"-->
<sp who="#la-tragedie">
                        <speaker>LA TRAGÉDIE.</speaker>
                        <l n="4" part="F">Moi, l'apôtre.</l><!--@syll="3"-->
                    </sp><!--@who="LA TRAGÉDIE"-->
<sp who="#la-comedie">
                        <speaker>LA COMÉDIE.</speaker>
                        <l n="5" part="I">Moi, l'abbé.</l><!--@syll="3"-->
                    </sp><!--@who="LA COMÉDIE"-->
<sp who="#la-tragedie">
                        <speaker>LA TRAGÉDIE.</speaker>
                        <l n="5" part="M">Le cothurne étoilé.</l><!--@syll="6"-->
                    </sp><!--@who="LA TRAGÉDIE"-->
<sp who="#la-comedie">
                        <speaker>LA COMÉDIE.</speaker>
                        <l n="5" part="F">Le sabot.</l><!--@syll="3"-->
                    </sp><!--@who="LA COMÉDIE"-->
<sp who="#la-tragedie">
                        <speaker>LA TRAGÉDIE.</speaker>
                        <l n="6" part="I">Le laurier.</l><!--@syll="3"-->
                    </sp><!--@who="LA TRAGÉDIE"-->
<sp who="#la-comedie">
                        <speaker>LA COMÉDIE.</speaker>
                        <l n="6" part="M">Le jambon.</l><!--@syll="3"-->
                    </sp><!--@who="LA COMÉDIE"-->
<sp who="#la-tragedie">
                        <speaker>LA TRAGÉDIE.</speaker>
                        <l n="6" part="M">Le sénat.</l><!--@syll="3"-->
                    </sp><!--@who="LA TRAGÉDIE"-->
<sp who="#la-comedie">
                        <speaker>LA COMÉDIE.</speaker>
                        <l n="6" part="F">Le turbot.</l><!--@syll="3"-->
                    </sp><!--@who="LA COMÉDIE"-->
<sp who="#la-tragedie">
                        <speaker>LA TRAGÉDIE.</speaker>
                        <l n="7" part="I">L'aveugle et le muet.</l><!--@syll="6"-->
                    </sp><!--@who="LA TRAGÉDIE"-->
<sp who="#la-comedie">
                        <speaker>LA COMÉDIE.</speaker>
                        <l n="7" part="F">Le myope et le bègue.</l><!--@syll="6"-->
                    </sp><!--@who="LA COMÉDIE"-->
<sp who="#la-tragedie">
                        <speaker>LA TRAGÉDIE.</speaker>
                        <l n="8" part="I">Catherine.</l><!--@syll="4"-->
                    </sp><!--@who="LA TRAGÉDIE"-->
<sp who="#la-comedie">
                        <speaker>LA COMÉDIE.</speaker>
                        <l n="8" part="M">Cateau.</l><!--@syll="2"-->
                    </sp><!--@who="LA COMÉDIE"-->
<sp who="#jupiter">
                        <speaker>JUPITER.</speaker>
                        <l n="8" part="M">Puis ?</l><!--@syll="1"-->
                    </sp><!--@who="JUPITER"-->
<sp who="#la-comedie">
                        <speaker>LA COMÉDIE.</speaker>
                        <l n="8" part="M">Géronte.</l><!--@syll="2"-->
                    </sp><!--@who="LA COMÉDIE"-->
<sp who="#la-tragedie">
                        <speaker>LA TRAGÉDIE.</speaker>
                        <l n="8" part="F">Don_Diègue.</l><!--@syll="3"-->
                    </sp><!--@who="LA TRAGÉDIE"-->
<sp who="#jupiter">
                        <speaker>JUPITER.</speaker>
                        <l n="9" part="I">Est-ce tout ?</l><!--@syll="3"-->
                    </sp><!--@who="JUPITER"-->
<sp who="#la-tragedie">
                        <speaker>LA TRAGÉDIE.</speaker>
                        <l n="9" part="M">Non.</l><!--@syll="1"-->
                    </sp><!--@who="LA TRAGÉDIE"-->
<sp who="#la-comedie">
                        <speaker>LA COMÉDIE.</speaker>
                        <l n="9" part="M">Nenni.</l><!--@syll="2"-->
                    </sp><!--@who="LA COMÉDIE"-->
<sp who="#la-tragedie">
                        <speaker>LA TRAGÉDIE.</speaker>
                        <l n="9" part="F">Je veux celui qui ment.</l><!--@syll="6"-->
                    </sp><!--@who="LA TRAGÉDIE"-->
<sp who="#la-comedie">
                        <speaker>LA COMÉDIE.</speaker>
                        <l n="10" part="I">Celui qui croit.</l><!--@syll="4"-->
                    </sp><!--@who="LA COMÉDIE"-->
<sp who="#la-tragedie">
                        <speaker>LA TRAGÉDIE.</speaker>
                        <l n="10" part="M">Le juge.</l><!--@syll="2"-->
                    </sp><!--@who="LA TRAGÉDIE"-->
<sp who="#la-comedie">
                        <speaker>LA COMÉDIE.</speaker>
                        <l n="10" part="F">Et moi, le jugement.</l><!--@syll="6"-->
                    </sp><!--@who="LA COMÉDIE"-->
<sp who="#la-tragedie">
                        <speaker>LA TRAGÉDIE.</speaker>
                        <l n="11" part="I">L'infini, l'absolu, l'immensité.</l><!--@syll="10"-->
                    </sp><!--@who="LA TRAGÉDIE"-->
<sp who="#la-comedie">
                        <speaker>LA COMÉDIE.</speaker>
                        <l n="11" part="F">Les bornes.</l><!--@syll="2"-->
                    </sp><!--@who="LA COMÉDIE"-->
<sp who="#la-tragedie">
                        <speaker>LA TRAGÉDIE.</speaker>
                        <l n="12" part="I">Ton aigle, ô Jupiter ! ta foudre, Ammon !</l><!--@syll="10"-->
                    </sp><!--@who="LA TRAGÉDIE"-->
<sp who="#la-comedie">
                        <speaker>LA COMÉDIE.</speaker>
                        <l n="12" part="F">Tes cornes.</l><!--@syll="2"-->
                    </sp><!--@who="LA COMÉDIE"-->
<sp who="#la-tragedie">
                        <speaker>LA TRAGÉDIE.</speaker>
                        <l n="13" part="I">Aller du Styx au ciel !</l><!--@syll="6"-->
                    </sp><!--@who="LA TRAGÉDIE"-->
<sp who="#la-comedie">
                        <speaker>LA COMÉDIE.</speaker>
                        <l n="13" part="F">De Paris à Saint-Cloud.</l><!--@syll="6"-->
                    </sp><!--@who="LA COMÉDIE"-->
<sp who="#la-tragedie">
                        <speaker>LA TRAGÉDIE.</speaker>
                        <l n="14" part="I">L'âpre forêt.</l><!--@syll="4"-->
                    </sp><!--@who="LA TRAGÉDIE"-->
<sp who="#la-comedie">
                        <speaker>LA COMÉDIE.</speaker>
                        <l n="14" part="M">Le bal.</l><!--@syll="2"-->
                    </sp><!--@who="LA COMÉDIE"-->
<sp who="#la-tragedie">
                        <speaker>LA TRAGÉDIE.</speaker>
                        <l n="14" part="M">Le grand lion.</l><!--@syll="4"-->
                    </sp><!--@who="LA TRAGÉDIE"-->
<sp who="#la-comedie">
                        <speaker>LA COMÉDIE.</speaker>
                        <l n="14" part="F">Le loup.</l><!--@syll="2"-->
                    </sp><!--@who="LA COMÉDIE"-->
<sp who="#la-tragedie">
                        <speaker>LA TRAGÉDIE.</speaker>
                        <l n="15" part="I">L'amour sur le sommet de l'Ida.</l><!--@syll="9"-->
                    </sp><!--@who="LA TRAGÉDIE"-->
<sp who="#la-comedie">
                        <speaker>LA COMÉDIE.</speaker>
                        <l n="15" part="F">Dans un fiacre.</l><!--@syll="3"-->
                    </sp><!--@who="LA COMÉDIE"-->
<sp who="#la-tragedie">
                        <speaker>LA TRAGÉDIE.</speaker>
                        <l n="16" part="I">Ève.</l><!--@syll="1"-->
                    </sp><!--@who="LA TRAGÉDIE"-->
<sp who="#la-comedie">
                        <speaker>LA COMÉDIE.</speaker>
                        <l n="16" part="M">Adam.</l><!--@syll="2"-->
                    </sp><!--@who="LA COMÉDIE"-->
<sp who="#la-tragedie">
                        <speaker>LA TRAGÉDIE.</speaker>
                        <l n="16" part="M">Le berger Pâris.</l><!--@syll="5"-->
                    </sp><!--@who="LA TRAGÉDIE"-->
<sp who="#la-comedie">
                        <speaker>LA COMÉDIE.</speaker>
                        <l n="16" part="F">Et moi, le diacre.</l><!--@syll="4"-->
                    </sp><!--@who="LA COMÉDIE"-->
<sp who="#la-tragedie">
                        <speaker>LA TRAGÉDIE.</speaker>
                        <l n="17" part="I">Le conquérant rebelle à Dieu.</l><!--@syll="8"-->
                    </sp><!--@who="LA TRAGÉDIE"-->
<sp who="#la-comedie">
                        <speaker>LA COMÉDIE.</speaker>
                        <l n="17" part="F">L'âne rétif.</l><!--@syll="4"-->
                    </sp><!--@who="LA COMÉDIE"-->
<sp who="#la-tragedie">
                        <speaker>LA TRAGÉDIE.</speaker>
                        <l n="18" part="I">L'imparfait de la vie.</l><!--@syll="6"-->
                    </sp><!--@who="LA TRAGÉDIE"-->
<sp who="#la-comedie">
                        <speaker>LA COMÉDIE.</speaker>
                        <l n="18" part="F">Et moi, du subjonctif.</l><!--@syll="6"-->
                    </sp><!--@who="LA COMÉDIE"-->
<sp who="#la-tragedie">
                        <speaker>LA TRAGÉDIE.</speaker>
                        <l n="19" part="I">La cloche.</l><!--@syll="3"-->
                    </sp><!--@who="LA TRAGÉDIE"-->
<sp who="#la-comedie">
                        <speaker>LA COMÉDIE.</speaker>
                        <l n="19" part="M">Le grelot.</l><!--@syll="3"-->
                    </sp><!--@who="LA COMÉDIE"-->
<sp who="#la-tragedie">
                        <speaker>LA TRAGÉDIE.</speaker>
                        <l n="19" part="F">Le pontife.</l><!--@syll="4"-->
                    </sp><!--@who="LA TRAGÉDIE"-->
<sp who="#la-comedie">
                        <speaker>LA COMÉDIE.</speaker>
                        <l n="19" part="F">Le pitre.</l><!--@syll="2"-->
                    </sp><!--@who="LA COMÉDIE"-->
<sp who="#la-tragedie">
                        <speaker>LA TRAGÉDIE.</speaker>
                        <l n="20" part="I">Les premiers temps des rois.</l><!--@syll="6"-->
                    </sp><!--@who="LA TRAGÉDIE"-->
<sp who="#la-comedie">
                        <speaker>LA COMÉDIE.</speaker>
                        <l n="20" part="F">Moi, le dernier chapitre.</l><!--@syll="6"-->
                    </sp><!--@who="LA COMÉDIE"-->
<sp who="#la-tragedie">
                        <speaker>LA TRAGÉDIE.</speaker>
                        <l n="21" part="I">Les mots sublimes dits par les grands.</l><!--@syll="9"-->
                    </sp><!--@who="LA TRAGÉDIE"-->
<sp who="#la-comedie">
                        <speaker>LA COMÉDIE.</speaker>
                        <l n="21" part="F">Les anas.</l><!--@syll="3"-->
                    </sp><!--@who="LA COMÉDIE"-->
<sp who="#la-tragedie">
                        <speaker>LA TRAGÉDIE.</speaker>
                        <l n="22" part="I">Les monstres marins noirs et terribles.</l><!--@syll="10"-->
                    </sp><!--@who="LA TRAGÉDIE"-->
<sp who="#la-comedie">
                        <speaker>LA COMÉDIE.</speaker>
                        <l n="22" part="F">Jonas.</l><!--@syll="2"-->
                    </sp><!--@who="LA COMÉDIE"-->
<sp who="#la-tragedie">
                        <speaker>LA TRAGÉDIE.</speaker>
                        <l n="23" part="I">Le héros.</l><!--@syll="3"-->
                    </sp><!--@who="LA TRAGÉDIE"-->
<sp who="#la-comedie">
                        <speaker>LA COMÉDIE.</speaker>
                        <l n="23" part="M">Le coquin de neveu.</l><!--@syll="6"-->
                    </sp><!--@who="LA COMÉDIE"-->
<sp who="#la-tragedie">
                        <speaker>LA TRAGÉDIE.</speaker>
                        <l n="23" part="F">Les quadriges.</l><!--@syll="3"-->
                    </sp><!--@who="LA TRAGÉDIE"-->
<sp who="#la-comedie">
                        <speaker>LA COMÉDIE.</speaker>
                        <l n="24" part="I">Les omnibus.</l><!--@syll="4"-->
                    </sp><!--@who="LA COMÉDIE"-->
<sp who="#la-tragedie">
                        <speaker>LA TRAGÉDIE.</speaker>
                        <l n="24" part="M">Moïse et Bouddha.</l><!--@syll="5"-->
                    </sp><!--@who="LA TRAGÉDIE"-->
<sp who="#la-comedie">
                        <speaker>LA COMÉDIE.</speaker>
                        <l n="24" part="F">Leurs prodiges.</l><!--@syll="3"-->
                        <stage type="signature">H.H. 26 juillet 1869.</stage>
                    </sp><!--@who="LA COMÉDIE"-->
</div>
</div>
</body>
